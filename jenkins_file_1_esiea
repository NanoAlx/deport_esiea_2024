pipeline{
    agent any
    
    stages {
        stage('Hello'){
            steps{
                echo 'Hello world'
            }
        }
        stage('Build'){
            steps{
                echo 'developpement'
            }
        }
        stage('Test'){
            steps{
                echo'Testing'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Mise en Production'
            }
        }
    }
}
